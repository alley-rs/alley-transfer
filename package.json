    "@tauri-apps/plugin-dialog": "2.0.0-beta.5",
    "@tauri-apps/plugin-os": "2.0.0-beta.5",
    "@tauri-apps/plugin-shell": "2.0.0-beta.6",
    "@tauri-apps/plugin-updater": "2.0.0-beta.5",
    "alley-components": "^0.2.2",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.17"
  },
  "devDependencies": {
    "@tauri-apps/cli": "2.0.0-beta.12",
    "@types/node": "^20.14.2",
    "@biomejs/biome": "^1.8.1",
    "cross-env": "^7.0.3",
