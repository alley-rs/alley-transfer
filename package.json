{
  "name": "alley",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "install:all": "bun i && cd static && bun i",
    "dev:vite": "vite",
    "build:vite": "tsc && vite build && cd static && tsc && vite build",
    "build": "tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "start:dev": "cross-env RUST_BACKTRACE=1 LOG_LEVEL=4 tauri dev",
    "trace": "cross-env RUST_BACKTRACE=1 LOG_LEVEL=5 tauri dev",
    "dev": "bun rune start:dev",
    "update:all": "bun run update && cd static && bun run update && cd ../src-tauri && cargo update",
    "build:android": "bun run tauri android build -t aarch64 x86_64 --split-per-abi"
  },
  "dependencies": {
    "@tauri-apps/api": "2.0.0-beta.11",
